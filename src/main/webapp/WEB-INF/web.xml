<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<!--suppress ALL -->
<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>UserController</servlet-name>
    <servlet-class>com.company.resume.UserController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UserController</servlet-name>
    <url-pattern>/userdetail</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>ErrorController</servlet-name>
    <servlet-class>com.company.resume.ErrorController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ErrorController</servlet-name>
    <url-pattern>/error</url-pattern>
  </servlet-mapping>

  <session-config>
    <session-timeout>
      30
    </session-timeout>
  </session-config>
</web-app>
